@startuml
'--------------------------------------------------------------------------------
'Script for Class Declaration
'--------------------------------------------------------------------------------
!include https://raw.githubusercontent.com/egrpy/SqlUMLSpec/main/sqlUMLSpecFunctions.puml

'--------------------------------------------------------------------------------
'Declare Label Class
'--------------------------------------------------------------------------------
!unquoted function $uumf_Label($LabelName="")
  !local $aux = ""
  !aux = $defClass($classType="", $name=$LabelName, $stereotype="<<label>>")
  !aux = $defClassAttribute($visibility = "-", $name="label", $dataType = ":string")
  !aux = $defClassEnd()
  !return $aux
!endfunction

@enduml
